<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.unknownuser.scrapstable">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".AddScrapsActivity"
                  android:label="Add Scraps Tracker">
        </activity>

        <activity android:name=".EditScrapsActivity"
                  android:label="Edit Scraps Tracker">
        </activity>

        <activity android:name=".ImageSelectActivity"
            android:label="Image Select">
        </activity>

        <receiver
            android:name=".MyAlarm"
            android:enabled="true"
            android:exported="true" />

        <service
            android:name=".EdibleService"
            android:exported="false"></service>
    </application>

</manifest>